<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="S7RTORRES.Login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ventana de Login"/>

            <Entry x:Name="txtUsuario" Placeholder="Ingrese usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese contrasena"/>
            <Button x:Name="btnIniciar" Text="Inicar sesion " Clicked="btnIniciar_Clicked"/>
            <Button x:Name="btnRegistrar" Text="Registrar" Clicked="btnRegistrar_Clicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>